    "sequelize-cli": "^5.5.1",
    "structure": "^1.8.0",
    "swagger-ui-express": "^4.1.2",
    "tedious": "^6.6.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-change": "^2.1.2",
